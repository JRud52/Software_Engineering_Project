<div class="container">

    @for (int i = 0; i < 5; i++)
    {
        <div class="row">
            <div class="col-md-4">
                <div class="row">
                    <div class="col-md-4"></div> <!-- this column empty -->
                    <div class="col-md-4"> @(7 * i + 1) </div>
                    <div class="col-md-4"> @(7 * i + 2) </div>
                </div>
            </div>

            <div class="col-md-8">
                <div class="row">
                    <div class="col-md-2"> @(7 * i + 3) </div>
                    <div class="col-md-2"> @(7 * i + 4) </div>
                    <div class="col-md-2"> @(7 * i + 5) </div>
                    <div class="col-md-2"> @(7 * i + 6) </div>
                    <div class="col-md-2"> @(7 * i + 7) </div>
                    <div class="col-md-2"></div> <!-- this column empty -->
                </div>
            </div>
        </div>
    }



</div>
